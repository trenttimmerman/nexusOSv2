{
  "functions": {
    "api/cron/send-scheduled-campaigns.ts": {
      "maxDuration": 300
    },
    "api/ai/generate-headers.ts": {
      "maxDuration": 60,
      "runtime": "nodejs20.x",
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/send-scheduled-campaigns",
      "schedule": "*/5 * * * *"
    }
  ]
}
